    default_applicable_licenses: ["Android-Apache-2.0"],
}

filegroup {
    name: "healthfitness-cts-testapp2-srcs",
    srcs: [
        "src/**/*.java",
    ],
    visibility: [
        "//packages/modules/HealthFitness:__subpackages__"
    ],
}


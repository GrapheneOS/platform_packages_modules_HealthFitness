        "android.test.base",
        "android.test.runner",
    ],

    srcs: [":healthfitness-cts-testapp2-srcs"],

    test_suites: [
        "device-tests",
        "general-tests",
    ],

    target_sdk_version: "34",
    min_sdk_version: "34",
}
